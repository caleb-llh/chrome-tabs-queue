{
    "manifest_version": 3,
    "name": "Tabs Queue Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension that reverses the order of your new tabs.",
    "permissions": ["tabs", "activeTab","storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16-queue.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }